{
  "name": "holocure-infos",
  "version": "1.0.0",
  "description": "Simple app to easily browse and visualize items and their possible combinations for [HoloCure](https://kay-yu.itch.io/holocure) (try it, it's free!)",
  "private": true,
  "workspaces": [
    "packages/admin-api",
    "packages/admin-web",
    "packages/web",
    "packages/common"
  ],
  "scripts": {
    "data": "node ./scripts/yaml-to-json.js",
    "start": "npm run dev",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:web": "npm run dev -w ./packages/web",
    "dev:admin-api": "npm run dev -w ./packages/admin-api",
    "dev:admin-web": "npm run dev -w ./packages/admin-web",
    "netlify": "npm run deploy -w ./packages/web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shabla/holocure-infos.git"
  },
  "author": "Shabla",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shabla/holocure-infos/issues"
  },
  "homepage": "https://github.com/shabla/holocure-infos#readme",
  "volta": {
    "node": "18.14.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "js-yaml": "~4.1.0"
  }
}
